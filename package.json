{
  "name": "app-name",
  "version": "0.1.0",
  "main": "server/dev",
  "description": "",
  "repository": "",
  "license": "",
  "scripts": {
    "test": "grommet check",
    "dev-server": "NODE_ENV=devserver nodemon ./server/dev",
    "dev": "NODE_ENV=development grommet pack",
    "dist": "grommet pack",
    "dist-server": "babel -d ./dist-server ./server -s",
    "start": "forever server/dev.js",
    "prod-start": "forever start -o /opt/mount1/passport-out.log -e /opt/mount1/passport-err.log server/dev.js"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "node-env-file": "^0.1.8",
    "query-string": "^4.3.1",
    "request": "^2.79.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-parallel": "^0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.9.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "sass-lint": "^1.10.2",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "pre-commit": [
    "test"
  ]
}
